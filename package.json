{
  "name": "serverless-app",
  "version": "0.1.0",
  "description": "Serverless framework starter template for ES6 with Babel, Webpack, and Jest testing",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "./node_modules/.bin/sls offline start -r us-east-1 --noTimeout",
    "install-dynamodb": "./node_modules/.bin/sls dynamodb install",
    "start-dynamodb": "./node_modules/.bin/sls dynamodb start -p 8000  --migrate true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prudhvirajmudunuri/serverlessApp.git"
  },
  "keywords": [
    "serverless",
    "lambda",
    "aws",
    "es6",
    "babel",
    "webpack",
    "jest"
  ],
  "author": "Prudhvi Raj",
  "license": "MIT",
  "homepage": "https://github.com/prudhvirajmudunuri/serverlessApp.git#readme",
  "devDependencies": {
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "jest": "16.0.2",
    "serverless-dynamodb-local": "^0.2.27",
    "serverless-offline": "^3.16.0",
    "serverless-webpack": "1.0.0-rc.2",
    "webpack": "1.13.2"
  },
  "dependencies": {
    "serverless-dynamodb-client": "0.0.2"
  }
}
